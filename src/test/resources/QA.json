{
  "executionDetails": {
    "appium_js_path": "/usr/local/bin/appium",
    "appium_node_path": "/usr/local/bin/node",
    "monitoring":true
  },
  "testFeed":[
    {
      "belongsTo":"User",
      "runsOn": "any",
      "appDir": "app",
      "nativeApp":true,
      "optimusDesiredCapabilities": {
        "appiumServerCapabilities": {
          "app": "qaDebug.apk",
          "platformName": "Android"
        },
        "androidOnlyCapabilities": {
          "appActivity": "org.simple.clinic.setup.SetupActivity",
          "appPackage": "org.simple.clinic.qa.debug",
          "uiautomator2ServerInstallTimeout": 120000,
          "unicodeKeyboard":true,
          "resetKeyboard":true,
          "useKeystore": false,
          "fullreset": true,
          "automationName": "UiAutomator2",
          "adbExecTimeout" :50000,
          "nativeWebScreenshot":true
        }
      },
      "deviceState": {
        "captureVideo": true
      }
    }
  ]
}
