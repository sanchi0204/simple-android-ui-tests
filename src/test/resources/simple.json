{
  "executionDetails": {
    "appium_js_path": "/usr/local/bin/appium",
    "appium_node_path": "/usr/local/bin/node",
    "monitoring":true
  },
  "testFeed":[
    {
      "belongsTo":"User",
      "runsOn": "any",
      "appDir": "app",
      "nativeApp":true,
      "optimusDesiredCapabilities": {
        "appiumServerCapabilities": {
          "app": "simpleQa.apk",
          "platformName": "Android"
        },
        "androidOnlyCapabilities": {
          "appActivity": "org.simple.clinic.activity.TheActivity",
          "appPackage": "org.simple.clinic.qa.debug",
          "avdLaunchTimeout": 300000,
          "unicodeKeyboard":false,
          "useKeystore": false
        }
      },
      "deviceState": {
        "captureVideo": true
      }
    }
  ]
}
